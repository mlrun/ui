{
  "name": "dashboard-react",
  "version": "0.1.0",
  "private": true,
  "homepage": "/mlrun",
  "dependencies": {
    "@monaco-editor/react": "^4.1.3",
    "axios": "^0.21.1",
    "camelcase": "^5.2.0",
    "chart.js": "^3.0.0-beta.11",
    "classnames": "^2.2.6",
    "concurrently": "^6.0.1",
    "cronstrue": "^1.103.0",
    "dagre": "^0.8.5",
    "dotenv": "6.2.0",
    "dotenv-expand": "5.1.0",
    "fs-extra": "7.0.1",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^1.1.0",
    "js-base64": "^2.5.2",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "pretty-bytes": "^5.3.0",
    "prismjs": "^1.19.0",
    "prop-types": "^15.7.2",
    "qs": "^6.9.6",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.4",
    "react-dev-utils": "^9.1.0",
    "react-dom": "^16.12.0",
    "react-flow-renderer": "^8.6.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-text-mask": "^5.4.3",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "resolve": "1.12.0",
    "semver": "6.3.0",
    "text-mask-addons": "^3.8.0",
    "uuid": "^8.2.0"
  },
  "scripts": {
    "start": "node scripts/start.js --no-cache",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "test:coverage": "npm run test -- --coverage --watchAll=false",
    "docker": "docker build -t ${MLRUN_DOCKER_REGISTRY}${MLRUN_DOCKER_REPO:-mlrun}/mlrun-ui:${MLRUN_DOCKER_TAG:-latest} --build-arg COMMIT_HASH=\"`git rev-parse --short HEAD`\" --build-arg DATE=\"`date -u`\" -f Dockerfile .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "mock-server": "node scripts/mockServer.js",
    "test:ui": "node scripts/testui.js",
    "report": "node tests/report.js",
    "test:regression": "npm run test:ui && npm run report",
    "start:regression": "concurrently \"npm:mock-server\" \"npm:start\" \"npm:test:regression\"",
    "ui-steps": "export BABEL_ENV=test; export NODE_ENV=test; npx -p @babel/core -p @babel/node babel-node --presets @babel/preset-env scripts/collectUITestsSteps.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/node": "^7.14.9",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@babel/register": "^7.13.14",
    "@cucumber/cucumber": "^7.0.0",
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-essentials": "^6.1.20",
    "@storybook/addon-links": "^6.1.20",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.20",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/react": "^11.0.2",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "acorn": "^7.4.1",
    "babel-eslint": "10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.6",
    "babel-node": "*",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "babel-plugin-jest-hoist": "^26.2.0",
    "babel-plugin-named-asset-import": "^0.3.4",
    "babel-plugin-prismjs": "^2.0.1",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-preset-react-app": "^9.0.2",
    "body-parser": "^1.19.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chai": "^4.3.4",
    "chromedriver": "^95.0.0",
    "crypto": "^1.0.1",
    "css-loader": "2.1.1",
    "cucumber-html-reporter": "^5.3.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-loader": "3.0.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "3.13.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "express": "^4.17.1",
    "file-loader": "3.0.1",
    "geckodriver": "^1.22.2",
    "html-webpack-plugin": "4.0.0-beta.5",
    "http-proxy-middleware": "^1.0.3",
    "jest": "24.9.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.0",
    "mini-css-extract-plugin": "0.8.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.5.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "7.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "prettier": "^1.19.1",
    "randexp": "^0.5.3",
    "react-scripts": "^1.0.7",
    "resolve-url-loader": "3.1.0",
    "sass-loader": "7.2.0",
    "selenium-webdriver": "^4.0.0-beta.2",
    "style-loader": "1.0.0",
    "stylelint": "^13.3.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.17.2",
    "terser-webpack-plugin": "1.4.1",
    "url-loader": "2.1.0",
    "webpack": "4.41.0",
    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "2.1.1",
    "workbox-webpack-plugin": "4.3.1"
  },
  "jest": {
    "verbose": true,
    "clearMocks": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/scripts/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-sixteen",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "babel": {
    "presets": [
      "react-app",
      "@babel/preset-react",
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-logical-assignment-operators",
      [
        "prismjs",
        {
          "languages": [
            "yaml",
            "json",
            "py"
          ],
          "theme": "coy",
          "css": true
        }
      ]
    ],
    "env": {
      "production": {
        "plugins": [
          [
            "react-remove-properties",
            {
              "properties": [
                "data-testid"
              ]
            }
          ]
        ]
      }
    }
  }
}
